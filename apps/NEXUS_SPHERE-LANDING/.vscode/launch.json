	"version": "0.2.0",
	"configurations": [
		{
			"name": "Landing: Dev Server",
			"request": "launch",
			"runtimeArgs": ["run", "debug"],
			"runtimeExecutable": "pnpm",
			"type": "node",
			"cwd": "${workspaceFolder}",
			"console": "integratedTerminal"
		},
		{
			"name": "Landing: Chrome Browser",
			"request": "launch",
			"type": "chrome",
			"url": "http://localhost:5173",
			"webRoot": "${workspaceFolder}/src"
		}
	]
}
